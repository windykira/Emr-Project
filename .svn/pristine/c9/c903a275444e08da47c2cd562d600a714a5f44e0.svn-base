.ztree li a.curSelectedNode {
	color: #333;
}
/* main */
.content {
	position: absolute;
	width: 100%;
	top: 51px;
	bottom: 51px;
	left: 0;
}
/* sidebar */
.sidebar,.sidebar2 {
	position: fixed;
	top: 51px;
	bottom: 51px;
	left: 0;
	z-index: 1000;
	display: block;
	width: 250px;
	padding: 5px;
	overflow: auto;
	background-color: #f5f5f5;
	border-right: 1px solid #eee;
}
.sidebar2 {
	top: 98px;
}
.sidebar .panel {
	margin: 0;
}
/* main */
.main, .main2 {
	position: absolute;
	overflow: auto;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	left: 250px;
	padding: 15px;
}
.main2 {
	top: 98px;
}
/* footer */
.footer {
	position: fixed;
	bottom: 0;
	width: 100%;
	height: 50px;
	padding: 10px 15px;
	line-height: 30px;
	background-color: #eee;
}
/* 右下角弹出窗 */
.pop-box {
	z-index: 100;
	position: absolute;
	bottom: 15px;
	right: 0;
	display: none;
	width: 100%;
	margin-bottom: 10px;
	padding: 15px;
}
.pop-box .panel {
	box-shadow: 0 0 5px 3px rgba(0,0,0,.1);
}
.pop-box .panel .close {
	text-shadow: 0 1px 0 #000;
	filter: alpha(opacity=70);
	opacity: .7;
}
.pop-box .table {
	margin-bottom: 0;
}

/* CHOSEN PLUGIN */
.chosen-container-single .chosen-single {
	background: #ffffff;
	box-shadow: none;
	-moz-box-sizing: border-box;
	background-color: #FFFFFF;
	border: 1px solid #CBD5DD;
	border-radius: 2px;
	cursor: text;
	height: auto !important;
	margin: 0;
	min-height: 30px;
	overflow: hidden;
	padding: 4px 12px;
	position: relative;
	width: 100%;
}

/* Validation */
label.error {
	color: #cc5965;
	display: inline-block;
	margin-left: 5px;
}
