<div class="main">
    <div class="well well-sm text-right">
        <button class="btn btn-primary btn-sm" type="button">书写病历</button>
        <button class="btn btn-primary btn-sm" type="button">查看病历</button>
    </div>
    <div class="panel panel-default">
        <div class="panel-body clearfix">
            <form class="form-inline">
                <div class="form-group form-group-sm">
                    <label for="">床号：</label>
                    <input type="text" class="form-control" placeholder="" value="" size="6">
                </div>
                &nbsp;
                <div class="form-group form-group-sm">
                    <label for="">住院号：</label>
                    <input type="text" class="form-control" placeholder="" value="" size="6">
                </div>
                &nbsp;
                <div class="form-group form-group-sm">
                    <label for="">姓名：</label>
                    <input type="text" class="form-control" placeholder="" value="" size="6">
                </div>
                &nbsp;
                <div class="form-group form-group-sm">
                    <label for="">状态：</label>
                    <select class="form-control">
                        <option value="">在科</option>
                        <option value="">转出</option>
                        <option value="">转入</option>
                    </select>
                </div>
                &nbsp;
                <div class="form-group form-group-sm">
                    <select class="form-control">
                        <option value="">我的病人</option>
                        <option value="" selected>科室病人</option>
                        <option value="">历史病人</option>
                    </select>
                </div>
                &nbsp;
                <button type="button" class="btn btn-primary btn-sm">查询</button>
            </form>
        </div>
    </div>
